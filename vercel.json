{
  "rewrites": [
    {
      "source": "/((?!api|_next|_static|_vercel|[\\w-]+\\.\\w+).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
